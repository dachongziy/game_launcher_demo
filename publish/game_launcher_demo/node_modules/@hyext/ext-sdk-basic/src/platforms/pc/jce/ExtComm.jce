module ExtComm
{
    enum ResCode
    {
        success = 0,
        paramError = 1,
        fail = 2,
        unkown = 3,
        loginError = 4,
        authorizeError = 5,
        objNotFound = 6,
        objAlreadyExist = 7,
        limitUpload = 8,
    };
	
	//公共请求头
    struct ExtCommonRequest
    {
        0 optional string componentTag;                   //类型tag
    };

    //公共响应头
    struct ExtCommonResponse
    {
        0 optional int res;                      //响应结果（参照ResCode）
        1 optional string msg;                   //描述
    };

};