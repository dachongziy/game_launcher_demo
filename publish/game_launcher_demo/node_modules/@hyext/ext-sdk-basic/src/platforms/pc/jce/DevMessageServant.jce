module DEV
{

    struct MessageHeader
    {
        0 optional string   messId;                          //消息id
        1 optional string   messType;                          //消息类型
        2 optional string   extUuid;                          //扩展uuid
        3 optional int   roomId;                          //房间号
        4 optional int   pushTime;                          //下发时间
    };
    
    struct MessageBody
    {
        0 optional string   event;                          //事件
        1 optional string   content;                          //事件内容
    };

    struct Message
    {
        0 optional MessageHeader   header;                          
        1 optional MessageBody   body;                          
    };
    
}; 