{
  "name": "ext-sdk-common",
  "isCommon": true,
  "version": "2.1.3",
  "buildTarget": "extsdk",
  "mainPath": "src",
  "releasePath": "release",
  "outputPath": "dist",
  "defaultModules": [],
  "baseModulesPath": "src/basicModules",
  "modulesPath": "src/modules",
  "platformsPath": "src/platforms",
  "buildEntryPath": "src/entry",
  "demoPath": "../../../hyext-sdk-demo",
  "buildTasks": {
    "rn": {
      "platform": {
        "name": "rn",
        "entry": "src/platforms/rn/index-com.js",
        "buildTool": "metro"
      },
      "npmConfig": {
        "name": "@hyext/ext-sdk-common"
      },
      "uaTags": [
        "ios",
        "mid_ios",
        "mid_zs_ios",
        "adr",
        "mid_adr",
        "mid_zs_adr"
      ]
    },
    "web": {
      "platform": {
        "name": "web",
        "entry": "src/platforms/web/index-com.js",
        "buildTool": "webpack"
      },
      "uaTags": [
        "web"
      ]
    },
    "pc": {
      "platform": {
        "name": "pc",
        "entry": "src/platforms/pc/index-com.js",
        "buildTool": "webpack"
      },
      "uaTags": [
        "pc_exe",
        "pc_exe_template"
      ]
    }
  }
}